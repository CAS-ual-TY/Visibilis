import de.cas_ual_ty.visibilis.Output;

public class Pi extends Node0to1
{
	public Pi(int posX, int posY)
	{
		super(posX, posY);
	}
	
	@Override
	protected float getValue()
	{
		return (float) Math.PI;
	}
}
