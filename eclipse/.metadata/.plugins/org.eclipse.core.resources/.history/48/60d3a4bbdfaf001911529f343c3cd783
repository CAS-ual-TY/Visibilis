package de.cas_ual_ty.mundusmagicus.magic.calculate;

import de.cas_ual_ty.mundusmagicus.magic.EnumMagicDataType;
import de.cas_ual_ty.mundusmagicus.magic.Input;
import de.cas_ual_ty.mundusmagicus.magic.Node;
import de.cas_ual_ty.mundusmagicus.magic.Output;
import de.cas_ual_ty.mundusmagicus.magic.general.Node2to1;

public class Root extends Node2to1
{
	public Root(int posX, int posY)
	{
		super(posX, posY);
	}
	
	@Override
	protected boolean canCalculate(float in1, float in2)
	{
		return in2 != 0F;
	}
	
	@Override
	protected float calculate(float in1, float in2)
	{
		return (float) Math.pow(Math.E, Math.log(in1) / in2);
	}
}
