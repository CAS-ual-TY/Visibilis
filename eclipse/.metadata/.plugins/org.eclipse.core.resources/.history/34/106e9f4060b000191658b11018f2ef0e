package de.cas_ual_ty.visibilis.function;

import de.cas_ual_ty.visibilis.EnumMagicDataType;
import de.cas_ual_ty.visibilis.Input;
import de.cas_ual_ty.visibilis.Node;
import de.cas_ual_ty.visibilis.Output;
import de.cas_ual_ty.visibilis.general.Node1to1;

public class Logarithm10 extends Node1to1
{
	public Logarithm10(int posX, int posY)
	{
		super(posX, posY);
	}
	
	@Override
	protected float calculate(float in1)
	{
		return (float) Math.log10(in1);
	}
}
