package de.cas_ual_ty.mundusmagicus.magic;

public class Connection
{
	/**
	 * The node the output is from
	 */
	public Node nodeOut;
	
	/**
	 * The node the output is from
	 */
	public Node nodeIn;
	
	/**
	 * The index of the output parameter
	 */
	public int indexOut;
	
	/**
	 * The index of the input parameter
	 */
	public int indexIn;
}
