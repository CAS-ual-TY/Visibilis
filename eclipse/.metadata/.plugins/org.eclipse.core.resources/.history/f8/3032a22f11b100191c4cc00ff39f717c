package de.cas_ual_ty.visibilis.calculate;

import de.cas_ual_ty.visibilis.EnumVDataType;
import de.cas_ual_ty.visibilis.Input;
import de.cas_ual_ty.visibilis.Node;
import de.cas_ual_ty.visibilis.Output;
import de.cas_ual_ty.visibilis.general.Node2to1;

public class FFModulus extends Node2to1
{
	public FFModulus(int posX, int posY)
	{
		super(posX, posY);
	}
	
	@Override
	protected boolean canCalculate(float in1, float in2)
	{
		return in2 != 0F;
	}
	
	@Override
	protected float calculate(float in1, float in2)
	{
		return in1 % in2;
	}
}
